  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [

    {
      "name": "Attach to Edge",
      "port": 4200,
      "request": "attach",
      "type": "msedge",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Launch Chrome",
      "request": "launch",
      "type": "chrome", //pwa-chrome
      "timeout": 20000,
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}"
    }

  ]
}
